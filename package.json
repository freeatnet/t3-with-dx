{
  "name": "t3-template-with-dx",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0 <19",
    "yarn": ">=1.22 <2"
  },
  "scripts": {
    "build": "next build",
    "clean": "rimraf .next node_modules/.cache src/server/edgedb/edgeql tsconfig.tsbuildinfo",
    "dev": "next dev",
    "edgedb": "edgedb",
    "edgedb:generate:all": "yarn edgedb:generate:types && yarn edgedb:generate:queries",
    "edgedb:generate:types": "npx @edgedb/generate edgeql-js --target esm --output-dir src/server/edgedb/edgeql/ --force-overwrite",
    "edgedb:generate:queries": "npx @edgedb/generate queries && yarn lint:fix:all",
    "lint": "SKIP_ENV_VALIDATION=1 next lint --report-unused-disable-directives=error",
    "lint:ci:all": "SKIP_ENV_VALIDATION=1 next lint --report-unused-disable-directives=error --max-warnings=0",
    "lint:fix:all": "SKIP_ENV_VALIDATION=1 next lint --report-unused-disable-directives=error --fix",
    "lint:precommit": "SKIP_ENV_VALIDATION=1 next lint --report-unused-disable-directives=error --fix --max-warnings=0",
    "precommit": "lint-staged",
    "prepare": "(husky install || (echo 'WARN: husky not installed' && exit 0))",
    "setup": "yarn edgedb:generate:all",
    "start": "next start",
    "tsc:typecheck": "SKIP_ENV_VALIDATION=1 tsc --noEmit --pretty --project tsconfig.json"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.4.1",
    "@tanstack/react-query": "^4.29.19",
    "@trpc/client": "^10.34.0",
    "@trpc/next": "^10.34.0",
    "@trpc/react-query": "^10.34.0",
    "@trpc/server": "^10.34.0",
    "clsx": "^1.2.1",
    "edgedb": "^1.3.2",
    "next": "^13.4.9",
    "next-auth": "^4.22.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "^1.12.4",
    "tailwindcss-debug-screens": "^2.2.1",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@edgedb/generate": "^0.3.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/eslint": "^8.37.0",
    "@types/lint-staged": "^13.2.0",
    "@types/node": "^18.16.19 <19",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.9",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.1.6"
  },
  "ct3aMetadata": {
    "initVersion": "7.14.0"
  }
}
